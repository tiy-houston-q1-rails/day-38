<h1>Sessions#new</h1>

<%= form_tag auth_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit_tag "Sign In", class: "btn btn-lg btn-primary" %>
  </div>
<% end %>
